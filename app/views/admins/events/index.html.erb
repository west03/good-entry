<div class="container">
  <h2 class="my-3">イベント一覧</h2>
  <table class="table">
    <thead>
      <tr>
        <th>タイトル</th>
        <th>ステータス</th>
        <th>場所</th>
        <th>日時</th>
        <th>参加人数</th>
      </tr>
    </thead>
    <tbody>
      <% @events.each do |events| %>
      <tr>
        <td><%= link_to events.title, admins_event_path(events.id) %></td>
        <td><%= events.holding_flag %></td>
        <th><%= events.prefecture %></th>
        <th><%= events.date_and_time %></th>
        <td><%= events.event_entries.count %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center my-3">
    <%#= paginate @events %>
  </div>
</div>
</div>